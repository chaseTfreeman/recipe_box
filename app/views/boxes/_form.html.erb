
<div class="form">
<% @box.errors.full_messages.each do |message| %>
<p>
  <%= message %>
</p>
<% end %>

<%= form_for @box do |f| %>
<h2>Create New box</h2>
<%= f.label :theme %>
<%= f.text_field :theme %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :img_url %>
  <%= f.text_field :img_url %>


  <%= f.submit %>
<% end %>
</div>
